    <div id="checkout-details">
      <span id="checkout-title">Your Pickup Order</span>
      <div class="address">
        <i class="fa-solid fa-location-dot"></i>
        <span>&nbsp;Pickup at 555 street abc 111.</span>
      </div>
      <div class="time">
        <i class="fa-solid fa-clock"></i>
